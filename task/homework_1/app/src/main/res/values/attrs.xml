<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EmojiView">
        <attr name="count" format="integer"/>
        <attr name="android:textSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="FlexBoxLayout">
        <attr name="innerPadding" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="MessageCardLayout">
        <attr name="android:src" format="reference"/>
        <attr name="username" format="string"/>
        <attr name="message" format="string"/>
        <attr name="android:background" format="reference"/>
        <attr name="titleTextAppearance" format="reference"/>
        <attr name="messageTextAppearance" format="reference"/>
        <attr name="defaultMargin" format="dimension"/>
        <attr name="imageSize" format="dimension"/>
    </declare-styleable>
</resources>